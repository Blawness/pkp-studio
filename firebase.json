{
  "emulators": {
    "dataconnect": {
      "dataDir": "dataconnect/.dataconnect/pgliteData"
    }
  },
  "dataconnect": {
    "source": "dataconnect"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "predeploy": [
        "npm --prefix "$RESOURCE_DIR" run lint",
        "npm --prefix "$RESOURCE_DIR" run build"
      ]
    },
    {
      "source": "pkp-database",
      "codebase": "pkp-database",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "runtime": "python313"
    },
    {
      "source": "database-pkp",
      "codebase": "database-pkp",
      "predeploy": [
        "npm --prefix "$RESOURCE_DIR" run lint",
        "npm --prefix "$RESOURCE_DIR" run build"
      ]
    },
    {
      "source": "database-pkp-app",
      "codebase": "database-pkp-app",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "runtime": "python313"
    }
  ],
  "apphosting": [
    {
      "backendId": "pkp-web-app",
      "rootDir": "/",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions"
      ]
    },
    {
      "backendId": "my-web-app",
      "rootDir": "/",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions"
      ]
    }
  ]
}